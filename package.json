{
  "name": "brainco-website-nextjs",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_ENV=production next build",
    "build:prod": "cross-env NODE_ENV=production next build && node scripts/upload-to-oss.js",
    "start": "next start",
    "upload:oss": "node scripts/upload-to-oss.js",
    "upload:oss:all": "node scripts/upload-to-oss.js --public",
    "clean:oss": "node scripts/clear-oss-cache.js --keep=3",
    "clean:oss:dry": "node scripts/clear-oss-cache.js --keep=3 --dry-run",
    "build-stats": "cross-env ANALYZE=true NODE_ENV=production npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "check:i18n": "i18n-check -l src/locales -s zh-CN -u src -f next-intl",
    "check:seo": "node scripts/check-seo-pages.mjs",
    "check:seo-headers": "node scripts/check-seo-headers.mjs",
    "test:seo": "./scripts/test-seo-features.sh",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:analyze": "node scripts/analyze-lighthouse.mjs",
    "crowdin:upload": "crowdin upload sources",
    "crowdin:download": "crowdin download",
    "crowdin:sync": "npm run crowdin:upload && npm run crowdin:download",
    "postbuild": "next-sitemap && node scripts/generate-sitemap.mjs"
  },
  "dependencies": {
    "@heroui/alert": "^2.2.27",
    "@heroui/system": "^2.4.23",
    "@heroui/theme": "^2.4.23",
    "@logtape/logtape": "^1.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.12",
    "@vant/area-data": "^2.1.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "gsap": "^3.13.0",
    "lucide-react": "^0.556.0",
    "marked": "^17.0.1",
    "motion": "^12.23.25",
    "next": "^16.0.7",
    "next-intl": "^4.5.8",
    "next-seo": "^6.7.0",
    "next-sitemap": "^4.2.3",
    "qrcode.react": "^4.2.0",
    "qs": "^6.13.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.68.0",
    "react-hot-toast": "^2.6.0",
    "react-vant": "^3.3.5",
    "sharp": "^0.34.5",
    "swiper": "^12.0.3",
    "tailwind-merge": "^3.4.0",
    "three": "^0.181.2",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@crowdin/cli": "^4.12.0",
    "@eslint-react/eslint-plugin": "^1.38.4",
    "@hookform/resolvers": "^3.10.0",
    "@lingual/i18n-check": "^0.8.14",
    "@next/bundle-analyzer": "^15.5.7",
    "@next/eslint-plugin-next": "^15.5.7",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^24.10.1",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.181.0",
    "ali-oss": "^6.21.0",
    "code-inspector-plugin": "^1.3.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "knip": "^5.71.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "xml2js": "^0.6.2"
  }
}
